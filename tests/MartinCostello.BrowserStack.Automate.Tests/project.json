{
  "authors": [ "Martin Costello" ],
  "copyright": "Martin Costello (c) 2015",
  "description": "Tests for MartinCostello.BrowserStack.Automate.",
  "title": "MartinCostello.BrowserStack.Automate.Tests",
  "version": "2.0.1-*",

  "buildOptions": {
    "additionalArguments": [ "/ruleset:../../MartinCostello.BrowserStack.Automate.ruleset", "/additionalfile:../../stylecop.json" ],
    "compile": {
      "includeFiles": [ "../../AssemblyVersion.cs", "../../CommonAssemblyInfo.cs" ]
    },
    "copyToOutput": {
      "include": [ "xunit.runner.json" ]
    },
    "keyFile": "../../MartinCostello.BrowserStack.Automate.snk",
    "warningsAsErrors": true,
    "xmlDoc": false
  },

  "dependencies": {
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "FluentAssertions": "4.15.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "MartinCostello.BrowserStack.Automate": {
      "target": "project"
    },
    "xunit": "2.2.0-beta3-build3402"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    },
    "net451": {
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1"
      }
    }
  },

  "packOptions": {
    "iconUrl": "https://martincostello.github.io/browserstack-automate/images/browserstack-logo.png",
    "licenseUrl": "https://github.com/martincostello/browserstack-automate/blob/master/LICENSE",
    "owners": [ "Martin Costello" ],
    "projectUrl": "https://github.com/martincostello/browserstack-automate",
    "releaseNotes": "See https://github.com/martincostello/browserstack-automate/releases for details.",
    "repository": {
      "type": "git",
      "url": "https://github.com/martincostello/browserstack-automate.git"
    },
    "requireLicenseAcceptance": false,
    "summary": "Tests for MartinCostello.BrowserStack.Automate.",
    "tags": []
  },

  "testRunner": "xunit"
}
